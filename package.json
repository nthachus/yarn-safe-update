{
  "name": "yarn-safe-update",
  "version": "1.0.0",
  "description": "Safety updating tool for packages in yarn.lock file",
  "author": "Thach Nguyen (https://github.com/nthachus)",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/nthachus/yarn-safe-update",
    "type": "git"
  },
  "main": "src/index.js",
  "bin": "./src/cli.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "lint": "node -r ./test-setup node_modules/eslint/bin/eslint.js . && node node_modules/flow-bin/cli.js check",
    "format": "node node_modules/prettier/bin-prettier.js --write \"**/*.{js,json,md}\" --ignore-path .eslintignore",
    "test": "node node_modules/mocha/bin/mocha -c -r ./test-setup -b -t 5000",
    "exec": "node -r ./test-setup src/cli.js",
    "build": "node node_modules/rimraf/bin.js dist && node node_modules/webpack/bin/webpack.js --progress",
    "clean": "node node_modules/rimraf/bin.js dist node_modules/.cache"
  },
  "dependencies": {
    "@yarnpkg/lockfile": "^1.1.0",
    "commander": "^2.20.0",
    "semver": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@babel/preset-env": "~7.9.0",
    "@babel/preset-flow": "~7.9.0",
    "@babel/register": "~7.9.0",
    "acorn": "^6.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "~8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "~14.1.0",
    "eslint-config-prettier": "~6.9.0",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "~2.20.1",
    "flow-bin": "0",
    "mocha": "^6.2.2",
    "object.entries": "^1.1.1",
    "prettier": "^1.19.1",
    "rimraf": "^2.6.3",
    "string-replace-loader": "~2.2.0",
    "terser-webpack-plugin": "^1.4.3",
    "webpack": "~4.42.0",
    "webpack-cli": "^3.3.12"
  },
  "resolutions": {
    "**/assert/util": "^0.11.0",
    "**/chalk/supports-color": "^6.1.0",
    "**/core-js-compat/semver": "^7.0.0",
    "**/debug/ms": "^2.1.1",
    "**/define-property/is-descriptor": "^1.0.0",
    "**/eslint-module-utils/pkg-dir": "^3.0.0",
    "**/inquirer/string-width": "^3.1.0",
    "**/read-pkg-up/find-up": "^3.0.0",
    "**/resolve-dir/global-modules": "^2.0.0",
    "**/wide-align/string-width": "^3.0.0",
    "mocha/glob": "^7.1.3",
    "mocha/supports-color": "^6.0.0",
    "webpack-cli/enhanced-resolve": "^4.1.0",
    "webpack/eslint-scope": "^5.0.0"
  },
  "prettier": {
    "trailingComma": "es5",
    "arrowParens": "always",
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "parser": "json-stringify"
        }
      }
    ]
  }
}
